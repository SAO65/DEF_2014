require(plyr)
# Read the raw dataset into R
data <- read.csv("/Users/stefanoolgiati/Desktop/Framework_2/DEF_2014/data.csv")
names(data)
head(data, 5)
# Analyse DPEF 2011 and DEF 2014
data.summary <- ddply(data, c("document", "entry", "status"), summarise,
mean = round(mean(value), 2),
sd  = round(sd(value), 2),
uplim = round(mean + 1.96 *sd, 2),
lowlim = round(mean - 1.96*sd, 2))
data.summary
# Generate data tables
dt <- data.table(data)
tables()
dt.gdp.dpef2011 <- dt[dt$document == "dpef2011" & dt$entry == "gdp",list(mean(value), sd(value))]
dt.gdp.dpef2011
dt.phe.dpef2011 <- dt[dt$document == "dpef2011" & dt$entry == "phe",list(mean(value), sd(value))]
dt.phe.dpef2011
dt.h.dpef2011 <- dt[dt$document == "dpef2011" & dt$entry == "h",list(mean(value), sd(value))]
dt.h.dpef2011
dt.gdp.def2014 <- dt[dt$document == "def2014" & dt$entry == "gdp",list(mean(value), sd(value))]
dt.gdp.def2014
dt.phe.def2014 <- dt[dt$document == "def2014" & dt$entry == "phe",list(mean(value), sd(value))]
dt.phe.def2014
dt.h.def2014 <- dt[dt$document == "def2014" & dt$entry == "h",list(mean(value), sd(value))]
dt.h.def2014
# Plot boxplots of gdp, phe and h
dt.gdp <- dt[dt$entry == "gdp"]
dt.gdp
dt.phe <- dt[dt$entry == "phe"]
dt.phe
dt.h <- dt[dt$entry == "h"]
dt.h
dt$document <- factor(dt$document, labels = c("dpef2011", "def2014"))
dt$document
dt$status <- factor(dt$status, labels = c("actual", "budget"))
dt$status
qplot(document, value, data = dt.gdp, main="GROSS DOMESTIC PRODUCT",
geom="boxplot")+ xlab("") + ylab("GDP") + geom_jitter(position=position_jitter(w=0,
h=0.3), color="grey") + facet_grid(.~status, scales="free") + theme(strip.text.y = element_text()) + geom_point() + theme_bw() + geom_boxplot(data=dt.gdp,
mapping=aes(x=document, y=value, color=document))
qplot(document, value, data = dt.phe, main="PUBLIC HEALTH EXPENDITURE",
geom="boxplot")+ xlab("") + ylab("Public Healthcare Expenditure") + geom_jitter(position=position_jitter(w=0,
h=0.3), color="grey") + facet_grid(.~status, scales="free") + theme(strip.text.y = element_text()) + geom_point() + theme_bw() + geom_boxplot(data=dt.phe, mapping=aes(x=document, y=value, color=document))
qplot(document, value, data = dt.h, main="PUBLIC HEALTH EXPENDITURE AS % OF GDP",
geom="boxplot")+ xlab("") + ylab("Public Healthcare Expenditure as % of GDP") + geom_jitter(position=position_jitter(w=0,
h=0.3), color="grey") + facet_grid(.~status, scales="free") + theme(strip.text.y = element_text()) + geom_point() + theme_bw() + geom_boxplot(data=dt.h, mapping=aes(x=document, y=value, color=document))
